(program
  (module Foo (import) (export () (toByteString takeByteString
         dropByteString toByteStringAndDrop))

    (declare toByteString (fun (integer) (bytestring)))
    (define  toByteString
      (lam x (builtin integerToByteString x)))

    (declare takeByteString (fun (integer) (fun (bytestring) (bytestring))))
    (define  takeByteString
      (lam x (lam y (builtin takeByteString x y))))

    (declare dropByteString (fun (integer) (fun (bytestring) (bytestring))))
    (define  dropByteString
      (lam x (lam y (builtin dropByteString x y))))

    (declare toByteStringAndDrop (fun (integer) (fun (integer) (bytestring))))
    (define  toByteStringAndDrop
      (lam x (lam y (builtin dropByteString x (builtin integerToByteString y)))))
  )
)
